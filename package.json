{
  "name": "@rhao/lodash-x",
  "type": "module",
  "version": "0.2.1",
  "description": "Extend generic functions based on lodash.",
  "author": "RayHao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/lodash-x",
  "repository": {
    "type": "git",
    "url": "https://github.com/l246804/lodash-x"
  },
  "bugs": {
    "url": "https://github.com/l246804/lodash-x/issues"
  },
  "keywords": [
    "lodash",
    "utils",
    "tools",
    "generic",
    "functions",
    "methods"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffect": false,
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "run-s gen:entry-file dev:lib",
    "dev:lib": "unbuild --stub",
    "build": "run-s gen:entry-file build:lib",
    "build:lib": "unbuild",
    "plop": "plop",
    "gen:entry-file": "node scripts/gen-entry-file.mjs",
    "cz": "cz",
    "release": "release-it"
  },
  "dependencies": {
    "@rhao/types-base": "^0.1.3",
    "@types/lodash": "^4.14.197",
    "@types/lodash-es": "^4.17.8",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@release-it/conventional-changelog": "^7.0.0",
    "@rhao/plop-generators": "^0.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.47.0",
    "eslint-define-config": "^1.23.0",
    "fs-extra": "^11.1.1",
    "git-cz": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "prettier": "^3.0.1",
    "release-it": "^16.1.5",
    "typescript": "^5.0.2",
    "unbuild": "^1.2.1"
  }
}
